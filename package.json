{
  "name": "zodiac-mapper",
  "version": "0.1.0",
  "description": "A lightweight, zero-dependency TypeScript NPM package that maps zodiac sign names in dozens of languages and regional variants to a standardized zodiac sign enum.",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run --coverage",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "zodiac-signs",
    "zodiac",
    "sign",
    "星座",
    "十二星座"
  ],
  "author": "fixicelo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fixicelo/zodiac-mapper.git"
  },
  "bugs": {
    "url": "https://github.com/fixicelo/zodiac-mapper/issues"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.25.0",
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@vitest/coverage-v8": "4.0.15",
    "tsdown": "^0.17.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
